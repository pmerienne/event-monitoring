<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:e="urn:import:com.pmerienne.eventmonitoring.client.widget.desktop.editor">
	<ui:style>
		
	</ui:style>

	<g:HTMLPanel>
		<form class="form-horizontal">
			<fieldset>
				<h2>Keys</h2>
				<g:HTMLPanel ui:field="keysContainer">
					<e:IndexKeyEditor />
				</g:HTMLPanel>
				<g:Button ui:field="addKeyButton" text="Add key" addStyleNames="btn" />

				<h2>Options</h2>
				<g:HorizontalPanel>
					<e:BooleanInput ui:field="uniqueInput" label="Unique" />
					<e:BooleanInput ui:field="dropDupsInput" label="Drop duplicates" />
				</g:HorizontalPanel>
			</fieldset>
		</form>
	</g:HTMLPanel>
</ui:UiBinder> 