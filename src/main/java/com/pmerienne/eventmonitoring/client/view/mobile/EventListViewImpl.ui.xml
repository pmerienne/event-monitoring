<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:mgwt="urn:import:com.googlecode.mgwt.ui.client.widget" xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:l="urn:import:com.pmerienne.eventmonitoring.client.widget.mobile.list" xmlns:b="urn:import:com.googlecode.mgwt.ui.client.widget.base">
	<ui:style>
		
	</ui:style>
	<mgwt:LayoutPanel>
		<mgwt:HeaderPanel>
			<mgwt:left>
				<mgwt:HeaderButton ui:field="backButton" text="Back" backButton="true" />
			</mgwt:left>

			<mgwt:center>
				<g:HTML ui:field="pageTitle" />
			</mgwt:center>
		</mgwt:HeaderPanel>

		<b:PullPanel ui:field="pullPanel">
			<mgwt:WidgetList ui:field="sortEntry">
				<mgwt:FormListEntry text="Sort by : ">
					<l:SortedFieldListBox ui:field="sortedFieldListBox" />
				</mgwt:FormListEntry>
			</mgwt:WidgetList>
		</b:PullPanel>
	</mgwt:LayoutPanel>
</ui:UiBinder> 