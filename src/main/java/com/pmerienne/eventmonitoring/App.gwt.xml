<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.3//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.3/distro-source/core/src/gwt-module.dtd">
<module rename-to="application">
	<!-- GWT -->
	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.activity.Activity" />
	<inherits name="com.google.gwt.editor.Editor" />

	<!-- GWT Highchart -->
	<inherits name="org.moxieapps.gwt.highcharts.Highcharts" />

	<!-- ANTLR for GWT -->
	<inherits name="org.antlr.AntLR" />

	<!-- Resty GWT -->
	<inherits name="org.fusesource.restygwt.RestyGWT" />

	<!-- set the user agent to safari -> only webkit -->
	<set-property name="user.agent" value="safari" />

	<entry-point class="com.pmerienne.eventmonitoring.client.Application" />

	<source path="client" />
	<source path="shared" />
</module>
